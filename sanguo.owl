#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/lihekun/SANGUO#Ancestor
sanguo:Ancestor rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( sanguo:Person
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty sanguo:isAncestorOf ;
                                                             owl:someValuesFrom sanguo:Person
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#Aunt
sanguo:Aunt rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ owl:intersectionOf ( sanguo:Person
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty sanguo:hasGender ;
                                                                                owl:hasValue sanguo:Female
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty sanguo:hasSibling ;
                                                         owl:someValuesFrom [ owl:intersectionOf ( sanguo:Person
                                                                                                   [ rdf:type owl:Restriction ;
                                                                                                     owl:onProperty sanguo:hasChild ;
                                                                                                     owl:someValuesFrom sanguo:Person
                                                                                                   ]
                                                                                                 ) ;
                                                                              rdf:type owl:Class
                                                                            ]
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#Child
sanguo:Child rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( sanguo:Person
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty sanguo:hasParent ;
                                                          owl:someValuesFrom sanguo:Person
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#Cousin
sanguo:Cousin rdf:type owl:Class ;
              rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#Daughter
sanguo:Daughter rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( sanguo:Person
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty sanguo:hasParent ;
                                                             owl:someValuesFrom sanguo:Person
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf sanguo:Child .


###  http://www.semanticweb.org/lihekun/SANGUO#DaughterInlaw
sanguo:DaughterInlaw rdf:type owl:Class ;
                     rdfs:subClassOf sanguo:Inlaw .


###  http://www.semanticweb.org/lihekun/SANGUO#Descendant
sanguo:Descendant rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( sanguo:Person
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty sanguo:hasAncestor ;
                                                               owl:someValuesFrom sanguo:Person
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#DomainEntity
sanguo:DomainEntity rdf:type owl:Class .


###  http://www.semanticweb.org/lihekun/SANGUO#FatherInLaw
sanguo:FatherInLaw rdf:type owl:Class ;
                   rdfs:subClassOf sanguo:ParentInLaw .


###  http://www.semanticweb.org/lihekun/SANGUO#FemaleAncestor
sanguo:FemaleAncestor rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( sanguo:Woman
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty sanguo:isAncestorOf ;
                                                                   owl:someValuesFrom sanguo:Person
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf sanguo:Ancestor .


###  http://www.semanticweb.org/lihekun/SANGUO#FemaleDescendant
sanguo:FemaleDescendant rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( sanguo:Woman
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty sanguo:hasAncestor ;
                                                                     owl:someValuesFrom sanguo:Person
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf sanguo:Descendant .


###  http://www.semanticweb.org/lihekun/SANGUO#Femaleness
sanguo:Femaleness rdf:type owl:Class ;
                  rdfs:subClassOf sanguo:Sex ;
                  owl:disjointWith sanguo:Maleness .


###  http://www.semanticweb.org/lihekun/SANGUO#Gender
sanguo:Gender rdf:type owl:Class ;
              rdfs:subClassOf sanguo:DomainEntity .


###  http://www.semanticweb.org/lihekun/SANGUO#Grandparent
sanguo:Grandparent rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( sanguo:Person
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty sanguo:isGrandparentOf ;
                                                                owl:someValuesFrom sanguo:Person
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#Inlaw
sanguo:Inlaw rdf:type owl:Class ;
             rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#MaleAncestor
sanguo:MaleAncestor rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( sanguo:Man
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty sanguo:isAncestorOf ;
                                                                 owl:someValuesFrom sanguo:Person
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf sanguo:Ancestor .


###  http://www.semanticweb.org/lihekun/SANGUO#MaleDescendant
sanguo:MaleDescendant rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( sanguo:Man
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty sanguo:hasAncestor ;
                                                                   owl:someValuesFrom sanguo:Person
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf sanguo:Descendant .


###  http://www.semanticweb.org/lihekun/SANGUO#Maleness
sanguo:Maleness rdf:type owl:Class ;
                rdfs:subClassOf sanguo:Sex .


###  http://www.semanticweb.org/lihekun/SANGUO#Man
sanguo:Man rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( sanguo:Person
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty sanguo:hasSex ;
                                                        owl:someValuesFrom sanguo:Maleness
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#MotherInLaw
sanguo:MotherInLaw rdf:type owl:Class ;
                   rdfs:subClassOf sanguo:ParentInLaw .


###  http://www.semanticweb.org/lihekun/SANGUO#ParentInLaw
