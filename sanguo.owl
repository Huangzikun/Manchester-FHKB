@prefix : <http://www.semanticweb.org/lihekun/SANGUO#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sanguo: <http://www.semanticweb.org/lihekun/SANGUO#> .
@base <http://www.semanticweb.org/lihekun/SANGUO> .

<http://www.semanticweb.org/lihekun/SANGUO> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/lihekun/SANGUO#hasAncestor
sanguo:hasAncestor rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sanguo:hasBloodrelation ;
                   owl:inverseOf sanguo:isAncestorOf ;
                   rdf:type owl:TransitiveProperty ;
                   owl:propertyChainAxiom ( sanguo:hasParent
                                          ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasAunt
sanguo:hasAunt rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf sanguo:hasBloodrelation ;
               owl:inverseOf sanguo:isAuntOf ;
               rdfs:range sanguo:Woman ;
               owl:propertyChainAxiom ( sanguo:hasParent
                                        sanguo:hasParent
                                        sanguo:hasDaughter
                                      ) ,
                                      ( sanguo:hasParent
                                        sanguo:hasSister
                                      ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasBloodrelation
sanguo:hasBloodrelation rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf sanguo:hasRelation .


###  http://www.semanticweb.org/lihekun/SANGUO#hasBrother
sanguo:hasBrother rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sanguo:hasSibling ;
                  owl:inverseOf sanguo:isBrotherOf ;
                  rdf:type owl:TransitiveProperty ;
                  rdfs:domain sanguo:Person ;
                  rdfs:range sanguo:Man ;
                  owl:propertyChainAxiom ( sanguo:hasParent
                                           sanguo:hasSon
                                         ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasChild
sanguo:hasChild rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sanguo:hasBloodrelation ;
                owl:inverseOf sanguo:isChildOf .


###  http://www.semanticweb.org/lihekun/SANGUO#hasCousin
sanguo:hasCousin rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sanguo:hasRelation ;
                 owl:inverseOf sanguo:isCousinOf ;
                 rdf:type owl:SymmetricProperty ;
                 owl:propertyChainAxiom ( sanguo:hasParent
                                          sanguo:hasSibling
                                          sanguo:hasChild
                                        ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasDaughter
sanguo:hasDaughter rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sanguo:hasChild ;
                   owl:inverseOf sanguo:isDaughterOf ;
                   rdfs:domain sanguo:Person ;
                   rdfs:range sanguo:Woman .


###  http://www.semanticweb.org/lihekun/SANGUO#hasDaughterInLaw
sanguo:hasDaughterInLaw rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf sanguo:hasInLaw ;
                        owl:inverseOf sanguo:isDaughterInLawOf ;
                        rdfs:domain sanguo:Person ;
                        owl:propertyChainAxiom ( sanguo:hasChild
                                                 sanguo:isFatherOf
                                                 sanguo:hasMother
                                               ) ,
                                               ( sanguo:hasSon
                                                 sanguo:hasSpouse
                                               ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasFather
sanguo:hasFather rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sanguo:hasParent ;
                 owl:inverseOf sanguo:isFatherOf ;
                 rdfs:domain sanguo:Person ;
                 rdfs:range sanguo:Man .


###  http://www.semanticweb.org/lihekun/SANGUO#hasFatherInLaw
sanguo:hasFatherInLaw rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sanguo:hasParentInLaw ;
                      owl:inverseOf sanguo:isFatherInLawOf ;
                      rdfs:domain sanguo:Person ;
                      rdfs:range sanguo:Man ;
                      owl:propertyChainAxiom ( sanguo:isSpouseOf
                                               sanguo:hasFather
                                             ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasGender
sanguo:hasGender rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/lihekun/SANGUO#hasGrandParent
sanguo:hasGrandParent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sanguo:hasAncestor ;
                      owl:inverseOf sanguo:isGrandParentOf ;
                      owl:propertyChainAxiom ( sanguo:hasGrandfather
                                             ) ,
                                             ( sanguo:hasGrandmother
                                             ) ,
                                             ( sanguo:hasParent
                                               sanguo:hasParent
                                             ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasGrandfather
sanguo:hasGrandfather rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sanguo:hasGrandParent ;
                      owl:propertyChainAxiom ( sanguo:hasParent
                                               sanguo:hasFather
                                             ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasGrandmother
sanguo:hasGrandmother rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sanguo:hasGrandParent ;
                      owl:propertyChainAxiom ( sanguo:hasParent
                                               sanguo:hasMother
                                             ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasGrandparent
sanguo:hasGrandparent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sanguo:hasBloodrelation .


###  http://www.semanticweb.org/lihekun/SANGUO#hasHusband
sanguo:hasHusband rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sanguo:hasSpouse .


###  http://www.semanticweb.org/lihekun/SANGUO#hasInLaw
sanguo:hasInLaw rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sanguo:hasRelation .


###  http://www.semanticweb.org/lihekun/SANGUO#hasMother
sanguo:hasMother rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sanguo:hasParent ;
                 owl:inverseOf sanguo:isMotherOf ;
                 rdfs:domain sanguo:Person ;
                 rdfs:range sanguo:Woman .


###  http://www.semanticweb.org/lihekun/SANGUO#hasMotherInLaw
sanguo:hasMotherInLaw rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sanguo:hasParentInLaw ;
                      owl:inverseOf sanguo:isMotherInLawOf ;
                      rdfs:domain sanguo:Person ;
                      rdfs:range sanguo:Woman ;
                      owl:propertyChainAxiom ( sanguo:isSpouseOf
                                               sanguo:hasMother
                                             ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasParent
sanguo:hasParent rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty sanguo:isChildOf ;
                 rdfs:subPropertyOf sanguo:hasAncestor ;
                 owl:inverseOf sanguo:isParentOf ;
                 rdfs:domain sanguo:Person ;
                 rdfs:range sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#hasParentInLaw
sanguo:hasParentInLaw rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sanguo:hasInLaw ;
                      owl:inverseOf sanguo:isParentInLawOf ;
                      rdfs:domain sanguo:Person ;
                      rdfs:range sanguo:Person ;
                      owl:propertyChainAxiom ( sanguo:isSpouseOf
                                               sanguo:hasParent
                                             ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasRelation
sanguo:hasRelation rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty .


###  http://www.semanticweb.org/lihekun/SANGUO#hasSex
sanguo:hasSex rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain sanguo:Person ;
              rdfs:range sanguo:Sex .


###  http://www.semanticweb.org/lihekun/SANGUO#hasSibling
sanguo:hasSibling rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sanguo:hasBloodrelation ;
                  rdf:type owl:SymmetricProperty ,
                           owl:TransitiveProperty .


###  http://www.semanticweb.org/lihekun/SANGUO#hasSister
sanguo:hasSister rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sanguo:hasSibling ;
                 owl:inverseOf sanguo:isSisterOf ;
                 rdf:type owl:TransitiveProperty ;
                 rdfs:domain sanguo:Person ;
                 rdfs:range sanguo:Woman ;
                 owl:propertyChainAxiom ( sanguo:hasParent
                                          sanguo:hasDaughter
                                        ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasSon
sanguo:hasSon rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf sanguo:hasChild ;
              owl:inverseOf sanguo:isSonOf ;
              rdfs:domain sanguo:Person ;
              rdfs:range sanguo:Man .


###  http://www.semanticweb.org/lihekun/SANGUO#hasSonInLaw
sanguo:hasSonInLaw rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sanguo:hasInLaw ;
                   owl:inverseOf sanguo:isSonInLawOf ;
                   rdfs:domain sanguo:Person ;
                   owl:propertyChainAxiom ( sanguo:hasChild
                                            sanguo:isMotherOf
                                            sanguo:hasFather
                                          ) ,
                                          ( sanguo:hasDaughter
                                            sanguo:hasSpouse
                                          ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasSpouse
sanguo:hasSpouse rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sanguo:hasRelation ;
                 owl:inverseOf sanguo:isSpouseOf .


###  http://www.semanticweb.org/lihekun/SANGUO#hasUncle
sanguo:hasUncle rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf sanguo:hasBloodrelation ;
                owl:inverseOf sanguo:isUncleOf ;
                rdfs:range sanguo:Man ;
                owl:propertyChainAxiom ( sanguo:hasParent
                                         sanguo:hasBrother
                                       ) ,
                                       ( sanguo:hasParent
                                         sanguo:hasParent
                                         sanguo:hasSon
                                       ) .


###  http://www.semanticweb.org/lihekun/SANGUO#hasVassal
sanguo:hasVassal rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sanguo:hasRelation ;
                 owl:inverseOf sanguo:isVassalOf ;
                 rdfs:domain sanguo:Person ;
                 rdfs:range sanguo:Person .


###  http://www.semanticweb.org/lihekun/SANGUO#hasWife
sanguo:hasWife rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf sanguo:hasSpouse .


###  http://www.semanticweb.org/lihekun/SANGUO#isAncestorOf
sanguo:isAncestorOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf sanguo:hasBloodrelation .


###  http://www.semanticweb.org/lihekun/SANGUO#isAuntOf
sanguo:isAuntOf rdf:type owl:ObjectProperty ;
